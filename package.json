{
  "name": "rayjay",
  "version": "0.2.0",
  "description": "Raytracer in One Weekend implementation",
  "author": "Jess Winter",
  "license": "MIT",
  "private": true,
  "type": "module",
  "keywords": [
    "raytracyer",
    "creative-code",
    "graphics"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -P . --noEmit",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "precommit": "pnpm lint:fix && pnpm format && pnpm typecheck"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
